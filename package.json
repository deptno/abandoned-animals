  "author": "bglee <deptno@gmail.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "--------------------훅": "#",
    "postinstall": "yarn build:packages",
    "--------------------빌드": "#",
    "build": "tsc -p packages/aa_datalake",
    "build:packages": "tsc -p packages/aa_assert -p packages/aa_type",
    "--------------------개발": "#",
    "graphql": "tsnd packages/aa_backend/handler/graphql.local.ts",
    "next": "cd packages/aa_frontend && next",
    "--------------------배포": "#",
    "dev:package": "cd packages/aa_backend && sls package -s dev",
    "dev:deploy": "cd packages/aa_backend && sls deploy -s dev",
    "--------------------로그": "#",
    "dev:log:graphql": "cd packages/aa_backend && sls logs -t -f graphql",
    "--------------------테스트": "#",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@types/aws-lambda": "^8.10.39",
    "@types/jest": "^24.0.25",
    "@types/node": "12",
    "@types/ramda": "^0.26.39",
    "@types/react": "^16.9.17",
    "apollo-server": "^2.9.16",
    "babel-loader": "^8.0.6",
    "dotenv": "^8.2.0",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^3.0.1",
    "serverless": "^1.60.5",
    "serverless-jetpack": "^0.7.0",
    "serverless-webpack": "^5.3.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.6.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.0-beta",
    "webpack": "^4.41.5"
  },
  "dependencies": {}
}
