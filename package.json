{
  "name": "abandoned-animals",
  "version": "0.0.0",
  "main": "index.js",
  "repository": "git@github.com:deptno/abandoned-animals.git",
  "author": "bglee <deptno@gmail.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "--------------------개발": "#",
    "graphql": "tsnd -r dotenv/config packages/aa_backend/handler/graphql.local.ts",
    "next": "cd packages/aa_frontend && next",
    "--------------------빌드": "#",
    "build": "yarn build:clean && yarn build:graphql && yarn build:packages",
    "build:clean": "rimraf packages/aa_assert/dist packages/aa_parser/dist packages/aa_data_source/dist packages/aa_type/dist",
    "build:graphql": "graphql-codegen --config codegen.yml",
    "build:packages": "tsc -p packages/aa_assert && tsc -p packages/aa_type && tsc -p packages/aa_parser && tsc -p packages/aa_data_source",
    "dev:build:frontend": "cd packages/aa_frontend && next build && next export && touch out/.nojekyll",
    "--------------------배포": "#",
    "dev:package:backend": "cd packages/aa_backend && sls package -s dev",
    "dev:deploy:backend": "cd packages/aa_backend && sls deploy -s dev",
    "dev:package:datalake": "cd packages/aa_datalake && sls package -s dev",
    "dev:deploy:datalake": "cd packages/aa_datalake && sls deploy -s dev",
    "dev:deploy:datalake:f:cronGetAbandonedAnimals": "cd packages/aa_datalake && sls deploy -s dev -f cronGetAbandonedAnimals",
    "--------------------로그": "#",
    "dev:log:graphql": "cd packages/aa_backend && sls -s dev -s dev logs -t -f graphql",
    "dev:log:cronGetAbandonedAnimals": "cd packages/aa_datalake && sls logs -s dev -t -f cronGetAbandonedAnimals",
    "dev:invoke:cronGetAbandonedAnimals": "cd packages/aa_datalake && sls invoke -s dev -f cronGetAbandonedAnimals",
    "dev:invoke:local:cronGetAbandonedAnimals": "cd packages/aa_datalake && sls invoke local -s dev -f cronGetAbandonedAnimals",
    "--------------------테스트": "#",
    "test": "jest",
    "test:aa_data_source": "jest aa_data_source",
    "test:aa_parser": "jest aa_parser",
    "--------------------기타": "#",
    "ws:remove:serverless-webpack": "yarn remove serverless-webpack -W",
    "ws:add:serverless-webpack": "yarn add serverless-webpack -D -W",
    "--------------------훅": "#",
    "postinstall": "yarn build"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@graphql-codegen/cli": "^1.11.2",
    "@graphql-codegen/introspection": "1.11.2",
    "@graphql-codegen/typescript": "1.11.2",
    "@graphql-codegen/typescript-operations": "^1.11.2",
    "@graphql-codegen/typescript-resolvers": "1.11.2",
    "@types/aws-lambda": "^8.10.39",
    "@types/jest": "^24.0.25",
    "@types/node": "12",
    "@types/ramda": "^0.26.39",
    "@types/react": "^16.9.17",
    "apollo-server": "^2.14.2",
    "babel-loader": "^8.0.6",
    "dotenv": "^8.2.0",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^3.0.1",
    "serverless": "^1.60.5",
    "serverless-jetpack": "^0.7.0",
    "serverless-webpack": "^5.3.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.6.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.0-beta",
    "webpack": "^4.41.5"
  },
  "dependencies": {}
}
